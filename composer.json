{
    "name": "manhattan/content-bundle",
    "type": "symfony-bundle",
    "description": "Easy integration of content bundle for Manhattan Content in Symfony2",
    "keywords": ["content", "layout"],
    "license": "MIT",
    "authors": [
        {
            "name": "James Rickard",
            "email": "james@frodosghost.com",
            "homepage": "http://frodosghost.com"
        }
    ],
    "autoload": {
        "psr-0": { "Manhattan\\Bundle\\ContentBundle": "" }
    },
    "target-dir": "Manhattan/Bundle/ContentBundle",
    "require": {
        "symfony/symfony": ">=2.3",
        "atom/logger-bundle": "1.0.*",
        "manhattan/console-bundle": ">=3.0,dev-master",
        "manhattan/publish-bundle": "dev-master"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    }
}
